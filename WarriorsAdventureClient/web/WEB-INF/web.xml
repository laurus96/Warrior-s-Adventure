<?xml version="1.0" encoding="UTF-8"?>
<web-app version="4.0" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd">
    <context-param>
        <param-name>javax.faces.PROJECT_STAGE</param-name>
        <param-value>Development</param-value>
    </context-param>
    <servlet>
        <servlet-name>Faces Servlet</servlet-name>
        <servlet-class>javax.faces.webapp.FacesServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>Servlet.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegistrationServlet</servlet-name>
        <servlet-class>Servlet.RegistrationServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CharacterLoadServlet</servlet-name>
        <servlet-class>Servlet.CharacterLoadServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CharacterCreationServlet</servlet-name>
        <servlet-class>Servlet.CharacterCreationServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AmministratorListPlayer</servlet-name>
        <servlet-class>Servlet.AmministratorListPlayer</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ToGame</servlet-name>
        <servlet-class>Servlet.ToGameServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UnbanServlet</servlet-name>
        <servlet-class>Servlet.UnbanServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>WeaponLoadServlet</servlet-name>
        <servlet-class>Servlet.WeaponLoadServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ArmorLoadServlet</servlet-name>
        <servlet-class>Servlet.ArmorLoadServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>Servlet.LogoutServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BoughtArmorServlet</servlet-name>
        <servlet-class>Servlet.BoughtArmorServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BoughtWeaponServlet</servlet-name>
        <servlet-class>Servlet.BoughtWeaponServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Faces Servlet</servlet-name>
        <url-pattern>/faces/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegistrationServlet</servlet-name>
        <url-pattern>/RegistrationServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CharacterLoadServlet</servlet-name>
        <url-pattern>/CharacterLoadServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CharacterCreationServlet</servlet-name>
        <url-pattern>/CharacterCreationServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AmministratorListPlayer</servlet-name>
        <url-pattern>/AmministratorListPlayer</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ToGame</servlet-name>
        <url-pattern>/ToGame</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UnbanServlet</servlet-name>
        <url-pattern>/UnbanServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>WeaponLoadServlet</servlet-name>
        <url-pattern>/WeaponLoadServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ArmorLoadServlet</servlet-name>
        <url-pattern>/ArmorLoadServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutServlet</servlet-name>
        <url-pattern>/LogoutServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BoughtArmorServlet</servlet-name>
        <url-pattern>/BoughtArmorServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BoughtWeaponServlet</servlet-name>
        <url-pattern>/BoughtWeaponServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>faces/index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
